      "source": [
        "data = []\n",
        "df1 = pd.read_csv('housing.csv')\n",
        "df2 = pd.read_csv('yachtData.csv')\n",
        "df3 = pd.read_csv('concreteData.csv')\n",
        "df1.name='Housing data'\n",
        "df2.name='Yacht Data'\n",
        "df3.name='Concrete Data'\n",
        "data.append(df1)\n",
        "data.append(df2)\n",
        "data.append(df3)"
